//	RRCA
//	RRCA
//	CPL
//	AND	00111000B
//	LD	B,A
//	LD	A,(HL)			; // HL = _FL_STOP_LP
//	RLCA
//	RLCA
//	RLCA
//	AND	00000111B
//	OR	B
//
